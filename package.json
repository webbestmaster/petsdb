    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.3.2",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-coverage-report": "^1.0.0",
    "typescript-eslint": "^7.15.0",
    "webpack": "^5.92.1",
